<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="text">
            <folder name="css">                
                <folder name="CompletionProviders">
                    <file name="com-liguorien-csscompletion-CSSCompletionProvider.instance"/>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="Services">
        <folder name="MIMEResolver">
            <file name="CSSResolver.xml" url="CSSResolver.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="com.liguorien.csseditor.Bundle"/>
            </file>
        </folder>    
    </folder>
    <folder name="Navigator">
        <folder name="Panels">           
            <folder name="text">
                <folder name="css">
                    <file name="com-liguorien-csscompletion-CSSNavigatorPanel.instance"/>                   
                </folder>
            </folder>
        </folder>
    </folder>
</filesystem>
